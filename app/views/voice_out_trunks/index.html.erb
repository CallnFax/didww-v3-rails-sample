<div class="panel panel-default">
  <div class="panel-heading">
    My Voice Out Trunks
  </div>
  <!-- /.panel-heading -->
  <div class="panel-body">
    <div class="row text-right">
      <div class="col-lg-12">
        <%= new_button new_voice_out_trunk_path %>
      </div>
    </div>
    <br>
    <div class="row">
      <div class="col-lg-12 table-responsive">
        <table width="100%" class="table table-striped table-bordered table-hover clickable">
          <thead>
            <tr>
              <th><%= t 'voice_out_trunk.name' %></th>
              <th><%= t 'voice_out_trunk.allowed_sip_ips' %></th>
              <th><%= t 'voice_out_trunk.allowed_rtp_ips' %></th>
              <th><%= t 'voice_out_trunk.on_cli_mismatch_action' %></th>
              <th><%= t 'voice_out_trunk.capacity_limit' %></th>
              <th><%= t 'voice_out_trunk.username' %></th>
              <th><%= t 'voice_out_trunk.password' %></th>
              <th><%= t 'voice_out_trunk.allow_any_did_as_cli' %></th>
              <th><%= t 'voice_out_trunk.status' %></th>
              <th><%= t 'voice_out_trunk.threshold_reached' %></th>
              <th><%= t 'voice_out_trunk.threshold_amount' %></th>
              <th><%= t 'voice_out_trunk.default_dst_action' %></th>
              <th><%= t 'voice_out_trunk.dst_prefixes' %></th>
              <th><%= t 'voice_out_trunk.media_encryption_mode' %></th>
              <th><%= t 'voice_out_trunk.callback_url' %></th>
              <th><%= t 'voice_out_trunk.force_symmetric_rtp' %></th>
              <th><%= t 'voice_out_trunk.rtp_ping' %></th>
              <th><%= t 'voice_out_trunk.spent_amount' %></th>
              <th><%= t 'voice_out_trunk.created_at' %></th>
              <th></th>
            </tr>
          </thead>
          <tbody>
            <%= render partial: 'voice_out_trunk', collection: resources %>
          </tbody>
        </table>
      </div>
    </div>
    <%= render 'shared/pagination' %>
  </div>
  <!-- /.panel-body -->
</div>
<!-- /.panel -->
